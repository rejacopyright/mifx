{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@src/*": [
        "src/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@api": [
        "src/api/index"
      ],
      "@api/*": [
        "src/api/*"
      ],
      "@redux/*": [
        "src/redux/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@helpers/*": [
        "src/helpers/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@styles/*": [
        "styles/*"
      ],
    }
  }
}